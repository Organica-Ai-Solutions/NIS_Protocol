{
    "family": "runner",
    "containerDefinitions": [
        {
            "cpu": 0,
            "environment": [
                {
                    "name": "AWS_REGION",
                    "value": "us-east-2"
                },
                {
                    "name": "AWS_SECRETS_ENABLED",
                    "value": "true"
                },
                {
                    "name": "KAFKA_BOOTSTRAP_SERVERS",
                    "value": "b-1.niskafka.iheb2p.c3.kafka.us-east-2.amazonaws.com:9092,b-2.niskafka.iheb2p.c3.kafka.us-east-2.amazonaws.com:9092"
                },
                {
                    "name": "REDIS_HOST",
                    "value": "nis-redis-standalone.d7vi10.ng.0001.use2.cache.amazonaws.com"
                },
                {
                    "name": "REDIS_PORT",
                    "value": "6379"
                },
                {
                    "name": "PYTHONUNBUFFERED",
                    "value": "1"
                },
                {
                    "name": "LOG_LEVEL",
                    "value": "INFO"
                }
            ],
            "essential": true,
            "image": "774518279463.dkr.ecr.us-east-2.amazonaws.com/nis-runner:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/runner",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "mountPoints": [],
            "name": "runner",
            "portMappings": [
                {
                    "containerPort": 8001,
                    "hostPort": 8001,
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::774518279463:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::774518279463:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}