{
    "family": "runner",
    "containerDefinitions": [
        {
            "cpu": 0,
            "environment": [
                {
                    "name": "PYTHONUNBUFFERED",
                    "value": "1"
                }
            ],
            "essential": true,
            "image": "774518279463.dkr.ecr.us-east-2.amazonaws.com/nis-runner:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/runner",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "mountPoints": [],
            "name": "runner",
            "portMappings": [
                {
                    "containerPort": 8001,
                    "hostPort": 8001,
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::774518279463:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::774518279463:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}